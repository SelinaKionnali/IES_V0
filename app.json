{
  "expo": {
    "name": "Kionnali",
    "slug": "IES_V0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kls.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/SplashScreen.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kionnali.iesv0",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            },
            "192.168.4.69": {
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/kls.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kionnali.lightform"
    },
    "web": {
      "favicon": "./assets/klsFavicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0cdb265f-8fe3-4033-bf32-44d89d8aa789"
      }
    }
  }
}